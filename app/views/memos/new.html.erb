<h1>new memo</h1>
<hr />

<%= bootstrap_form_for @memo do |f| %>
    <%# FIXME: 현재 유저의 아이디 가져오기 %>
    <%= f.hidden_field :user_id, value: @memo.user_id %>
    <%= f.text_field :title, label: "제목" %>
    <%= f.text_area :content, :class => "tinymce", :rows => 15, :cols => 120 %>
    <%= tinymce %>
    <%= f.submit %>
<% end %>